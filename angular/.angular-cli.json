{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "silveredgenew"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss",
		"assets/css/vendor.min.css",
		"assets/css/styles.css",
		"assets/plugins/perfect-scrollbar/css/perfect-scrollbar.css",
		"assets/plugins/rickshaw/rickshaw.min.css",
		"assets/plugins/bootstrap-select/css/bootstrap-select.css",
		"assets/plugins/amcharts/export.css",
		"assets/plugins/sweet_alert/sweetalert.css",
		"assets/plugins/fullcalendar/fullcalendar.print.css",
		"assets/plugins/fullcalendar/fullcalendar.min.css"
      ],
      "scripts": [
		"assets/js/modernizr.min.js",
		"assets/js/vendor.min.js",
		"assets/js/scripts.min.js",
		"assets/plugins/chartjs/Chart.min.js",
		"assets/plugins/moment/moment.min.js",
		"assets/plugins/perfect-scrollbar/js/perfect-scrollbar.jquery.min.js",
		"assets/plugins/d3/d3.js",
		"assets/plugins/rickshaw/rickshaw.min.js",
		"assets/plugins/ResizeSensor/ResizeSensor.js",
		"assets/plugins/jquery.flot/jquery.flot.js",
		"assets/plugins/jquery.flot/jquery.flot.resize.js",
		"assets/plugins/jquery.flot/jquery.flot.spline.js",
		"assets/plugins/jquery.sparkline/jquery.sparkline.min.js",
		"assets/plugins/bootstrap-select/js/bootstrap-select.js",
		"assets/plugins/jquery-validate/jquery.validate.min.js",
		"assets/plugins/jquery-validate/additional-methods.min.js",
		"assets/plugins/jquery-validate/jquery-validate.bootstrap-tooltip.min.js",
		"assets/plugins/jquery.sparkline/jquery.sparkline.min.js",
		"assets/plugins/chartjs/Chart.min.js",
		"assets/plugins/sweet_alert/sweetalert.min.js",
		"assets/plugins/fullcalendar/lib/moment.min.js",
		"assets/plugins/fullcalendar/fullcalendar.min.js"
	  ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
